set term svg background 'white'
set xlabel 'x'
set ylabel 'y'
set title 'Implementation of artificial neural network using 30 hidden neurons'
set grid xtics
set grid ytics
set xrange [-4:4]
plot '1Ddata.txt' u 1:3 w l lw 2 linecolor rgb 'black' t 'ANN'\
	,'1Ddata.txt' u 1:2 w l lw 4 linecolor rgb 'red' dashtype (10,20) t 'erf(x)'\
	,'1Ddata.txt' u 1:4 w l lw 2 t 'derivative of erf(x)'
