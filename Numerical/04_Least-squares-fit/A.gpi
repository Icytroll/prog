set term svg background 'white'
set xlabel 'x'
set ylabel 'F(x)'
set grid xtics
set grid ytics
set title 'Least squares solution using QR decomp and the functions: log(x), 1, x'
set key bottom right
plot 'input.data' u 1:2:3 w errorbars title 'Input data' \
	,'A.txt' u 1:2 w l title 'F(x)' \
	,'A.txt' u 1:3 w l title 'F(x) - dF(x)' \
	,'A.txt' u 1:4 w l title 'F(x) + dF(x)'
